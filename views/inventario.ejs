<br><br>

<h2>Inventario de productos</h2>
<table>
    <tr>
        <th>id</th>
        <th>Producto</th>
        <th>Descripción</th>
        <th>Stock</th>
        <th>Ubicación</th>
        <th>Fecha de Vencimiento</th>
        <th>Es kit</th>
        <!-- <th>Valor</th> -->
    </tr>
    <% prod.forEach(function(prod) { %>
        <tr>
            <td><%= prod.id_pro %></td>
            <td><%= prod.nombre_pro %></td>
            <td><%= prod.descripcion_pro %></td>
            <td><%= prod.cantidad_pro %></td>
            <td><%= prod.ubicacion_pro %></td>
            <td><%= new Date(prod.fecha_venc).toLocaleDateString() %></td>
            <td><%= prod.kit_pro %></td>
            <!-- <td><%= prod.precio_pro %></td> -->
        </tr>
    <% }); %>
</table>
<a href="/">Index</a>

